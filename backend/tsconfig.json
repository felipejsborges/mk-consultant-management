{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "strictPropertyInitialization": false,
		"baseUrl": "./src",
    "paths": {
      "@modules/*": ["modules/*"],
			"@config/*": ["config/*"]
    },
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
